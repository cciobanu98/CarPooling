@{
    ViewBag.Title = "Paputka";
    int? number = ViewData["Count"] as int?;
    int tpages = (int)number / 10;
    Layout = "_Layout";
}
<section class="main-content">
    <div class="container">
        <div class="row">
            <div class="page-content">
                <div class="clearfix"></div>
                <div class="last-rides">
                    <div class="col-md-12 col-sm-12 col-xs-12">
                        <div class="page-sub-title textcenter">
                            <h2>RIDES</h2>
                            <div class="line"></div>
                        </div><!-- end .page-sub-title -->
                    </div>
                    <article>
                        <div class="ride-content">
                            <h3><input id="SearchRidesAll" type="text" placeholder="Search" /></h3>
                        </div>
                        <div class="container" style="margin-left:-90px">
                            <ul class="ride-meta">
                                <li class="ride-date">
                                    <a id="SortDateAll" class="tooltip-link" dara-original0title="Date" data-toggle="tooltip">
                                        <i class="fa fa-calendar"></i>
                                        Date
                                        <i class="fa fa-sort" aria-hidden="true"></i>
                                    </a>
                                </li>
                                <li class="ride-people" name="ascending">
                                    <a id="SortSeatsAll" class="tooltip-link" dara-original-title="Date" data-toggle="tooltip">
                                        <i class="fa fa-user"></i>
                                        Seats
                                        <i class="fa fa-sort" aria-hidden="true"></i>
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </article>
                    <div class="col-md-12 col-sm-12 col-xs-12" id="rides-list">
                        <div id="AllRideTable"></div>
                        <div class="clearfix"></div>
                    </div><!-- end .col-md-12 col-sm-12 col-xs-12 -->
                </div><!-- end .last-rides -->

            </div>
        </div>
    </div>
</section>
<div id="ModalMoreRead"></div>
<script src="~/scripts/Account.js"></script>